    "babel-loader": "^8.0.0",
    "babel-preset-env": "^1.7.0",
    "documentation": "^8.1.2",
    "eslint": "^5.6.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-prettier": "^2.6.2",
    "flow-bin": "^0.81.0",
